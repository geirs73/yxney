<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="15.0" DefaultTargets="Build" xmlns="http://schemas.microsoft.com/developer/msbuild/2003" InitialTargets="">
  <Target Name="Build" DependsOnTargets="$(DefaultTargets)"></Target>
  
  <PropertyGroup>
    <DefaultTargets>
      PrintStandardProperties;
      PrintBuildLocations
    </DefaultTargets>
  </PropertyGroup>

  <Target Name="PrintStandardProperties">
    <Message Text="MSBuildBinPath $(MSBuildBinPath)"/>
    <Message Text="MSBuildExtensionsPath $(MSBuildExtensionsPath)"/>
    <Message Text="MSBuildExtensionsPath32 $(MSBuildExtensionsPath32)"/>
    <Message Text="MSBuildExtensionsPath64 $(MSBuildExtensionsPath64)"/>
    <Message Text="MSBuildProjectDefaultTargets $(MSBuildProjectDefaultTargets)"/>
    <Message Text="MSBuildProjectDirectory $(MSBuildProjectDirectory)"/>
    <Message Text="MSBuildProjectExtension $(MSBuildProjectExtension)"/>
    <Message Text="MSBuildProjectFile $(MSBuildProjectFile)"/>
    <Message Text="MSBuildProjectFullPath $(MSBuildProjectFullPath)"/>
    <Message Text="MSBuildProjectName $(MSBuildProjectName)"/>
    <Message Text="MSBuildStartupDirectory $(MSBuildStartupDirectory)"/>
    <Message Text="MSBuildToolsPath $(MSBuildToolsPath)"/>  
  </Target>

  <Target Name="PrintBuildLocations">
    <Message Text="$(MSBuildToolsPath)\Microsoft.CSharp.targets"/>
    <Message Text="$(MSBuildExtensionsPath)\$(MSBuildToolsVersion)\Microsoft.Common.props"/>

  </Target>

</Project>