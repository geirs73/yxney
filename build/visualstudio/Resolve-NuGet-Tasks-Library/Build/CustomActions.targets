<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003" InitialTargets="">
  <PropertyGroup>
    <BuildDependsOn>
      LocalBeforeBuild;
      $(BuildDependsOn);
    </BuildDependsOn>
  </PropertyGroup>
  <Target Name="LocalBeforeBuild">
    <Message Text="Before-Sleep" />
    <Sleep Milliseconds="5000" />
    <Message Text="After-Sleep" />    
  </Target>
</Project>
